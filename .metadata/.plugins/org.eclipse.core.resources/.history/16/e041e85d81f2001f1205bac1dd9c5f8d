package day07.practice;

public class 함수문제1번 {

	public static void main(String[] args) {
		
		//1. 수배열을 매개변수로 짝수의 개수를 반환하는 함수를 만들기
		int[] nums = {1,23,65,4,52,16,32};
		
	}

	
	public static void 짝수개수(int[] nums) {
		for(int i=0; i<nums.length; i++) {
			if(nums[i]%2==0) {
				int cnt=0;
				cnt++; 
			}
		}
	}
}
